{
  "version": 2,
  "functions": {
    "api/generate-music.js": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/try-it-now",
      "destination": "/try-it-now.html"
    },
    {
      "source": "/waitlist",
      "destination": "/waitlist.html"
    },
    {
      "source": "/cost-preview-demo",
      "destination": "/cost-preview-demo.html"
    },
    {
      "source": "/async-polling-demo",
      "destination": "/async-polling-demo.html"
    },
    {
      "source": "/admin-test",
      "destination": "/admin-test.html"
    }
  ]
}
