{
  "version": 2,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/try-it-now",
      "destination": "/try-it-now.html"
    },
    {
      "source": "/waitlist",
      "destination": "/waitlist.html"
    }
  ]
}
